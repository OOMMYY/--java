package greedy;

public class JS {
	int n=7;
	int [] D={0,2,3,1,3,4,5,2};//下标从1开始
	float[] P={0,15,12,11,10,8};//先对P进行非增排序
	int[] J=new int[n+1]; 
	int k;
	JS(){}
	public int js(float[] D,float[] J,int n,int k){
		int i,j,r;
		k=1;
		J[1]=1;//把第一个作业加入
		for(i=2;i<n;i++){
			r=k;
			while(D[J[r]]>D[i]&&D[J[r]]!=r){
				
			}
		}
		return k;
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		new JS();
	}

}
